<model tag="numudis">
    <rule index="0" name="No Osc"/>
    <rule index="1" name="Numu Dis"/>
    <rule index="2" name="Nue App"/>
</model>

<MCFile treename="events/selectedNu" filename="selected_events_dependentdetsysts.root" scale = "1.0" maxevents="50000" pot="1"> 
    <friend treename="events/multisimTree" filename="selected_events_dependentdetsysts.root"/>
    <friend treename="events/multisigmaTree" filename="selected_events_dependentdetsysts.root"/>
     <branch
         name                  = "recoE"
         associated_subchannel = "nu_SBND_numu_cc"
         oscillate             = "true"
         model_rule            = "1"
         true_param_name       = "trueE" 
         true_L_name           = "trueL / 1000"
         pdg_name              = "truePDG"
         additional_weight     = "CC"
         />
</MCFile>

<MCFile treename="events/selectedCos" filename="selected_events_dependentdetsysts.root" scale = "1.0" maxevents="50000" pot="1"> 
     <branch
         name                  = "recoE"
         associated_subchannel = "nu_SBND_numu_cosmic"
         incl_systematics      = "false"
         oscillate             = "false"
         model_rule            = "0"
         true_param_name       = "0" 
         true_L_name           = "0"
         pdg_name              = "0"
         additional_weight     = "1.0"
         />
</MCFile>


